<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <PreferenceCategory
        android:key="@string/settings_Google"
        android:title="@string/settings_Google">
        <!-- Usuario de Twitter -->
        <EditTextPreference
            android:key="usuario_google"
            android:title="@string/settings_usuario_tituloG"
            android:summary="@string/settings_usuario_resumenG"
            android:selectAllOnFocus="true"
            android:singleLine="true" />

        <!-- Password de Twitter -->
        <EditTextPreference
            android:key="contraseña_google"
            android:title="@string/settings_contraseña_tituloG"
            android:summary="@string/settings_contraseña_resumenG"
            android:selectAllOnFocus="true"
            android:inputType="textWebPassword"
            android:singleLine="true" />
    </PreferenceCategory>
