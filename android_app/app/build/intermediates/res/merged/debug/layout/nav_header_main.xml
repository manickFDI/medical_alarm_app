<?xml version="1.0" encoding="utf-8"?>

<!-- Contiene el header del navigation: foto y texto(titulo) -->
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:src="@drawable/background3"
        android:scaleType="centerCrop" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/medical"
        android:textAppearance="@style/TextAppearance.AppCompat.Large.Inverse"
        android:textStyle="bold"
        android:layout_gravity="bottom"
        android:layout_marginBottom="10dp"
        android:layout_marginLeft="10dp" />

</FrameLayout><!-- From: file:/D:/My%20downloads/medical_alarm_app-diego_prueba/android_app/app/src/main/res/layout/nav_header_main.xml -->